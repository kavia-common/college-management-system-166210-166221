# Copy to .env and set values. Do NOT commit real secrets.
SECRET_KEY=change-me

# Token expiry in minutes
ACCESS_TOKEN_EXPIRES_MINUTES=60

# CORS origins (comma-separated or * for any)
CORS_ORIGINS=*

# MySQL connection (provided by database container in deployment)
MYSQL_URL=127.0.0.1
MYSQL_USER=root
MYSQL_PASSWORD=Password123!
MYSQL_DB=college_db
MYSQL_PORT=3306

# Alternatively, you can provide a full SQLAlchemy database URL (overrides MySQL vars)
# DATABASE_URL=sqlite:///college.db
